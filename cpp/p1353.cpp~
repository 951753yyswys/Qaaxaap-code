#include<bits/stdc++.h>
#define int long long
using namespace std;
int d[100100],dp[10100][510];
signed main()
{
	int n,m;
	cin>>n>>m;
	for(int i=1;i<=n;i++) cin>>d[i];
	for(int i=0;i<=n;i++) for(int j=1;j<=m;j++) dp[i][j]=-0x7fffffffffffff;
	dp[0][0]=0;
	for(int i=0;i<=n;i++) 
	{
		for(int j=0;j<=m;j++)
		{
			dp[i+1][j+1]=max(dp[i+1][j+1],dp[i][j]+d[i]);
			dp[i+j][0]=max(dp[i+j][0],dp[i][j]);
		}
	}
	cout<<dp[n][0];
	return 0;
}
