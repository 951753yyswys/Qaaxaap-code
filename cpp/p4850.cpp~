#include<bits/stdc++.h>
#define int long long
using namespace std;
int a[101][101],sum[101][101],vis[51][51][51][51];
inline int qu(int x1,int y1,int x2,int y2)
{
	if(x1>x2) swap(x1,x2);
	if(y1>y2) swap(y1,y2);
	return sum[x2][y2]-sum[x1-1][y2]-sum[x2][y1-1]+sum[x1-1][y1-1];
}
int dfs(int x1,int y1,int x2,int y2)
{
	if(x1==x2&&y1==y2) return a[x1][y1];
	if(vis[x1][y1][x2][y2]!=0) return vis[x1][y1][x2][y2];
	int ans=0x7fffffff;
	for(int i=x1;i<x2;i++)
		ans=min(ans,dfs(x1,y1,i,y2)+dfs(i+1,y1,x2,y2));
	for(int i=y1;i<y2;i++)
		ans=min(ans,dfs(x1,y1,x2,i)+dfs(x1,i+1,x2,y2));
	ans+=qu(x1,y1,x2,y2);
	vis[x1][y1][x2][y2]=ans;
	cout<<"(x1,y1),(x2,y2),ans"<<x1<<' '<<y1<<' '<<x2<<' '<<y2<<' '<<ans<<endl;
	return ans;
}
signed main()
{
	int n,m;
	cin>>n>>m;
	for(int i=1;i<=n;i++) for(int j=1;j<=m;j++) cin>>a[i][j];
	for(int i=1;i<=n;i++) for(int j=1;j<=m;j++)
		sum[i][j]=sum[i-1][j]+sum[i][j-1]+a[i][j]-sum[i-1][j-1];
	cout<<dfs(1,1,n,m)-qu(1,1,n,m);
	return 0;
 } 
