#include<bits/stdc++.h>
#define int long long
#define ull unsigned long long	
using namespace std;
namespace Qaaxaap
{
	const int N=5e5+5,mod=998244353,len=101;
	ull qp(ull d,int z)
	{
		ull t;
		while(z)
		{
			if(z&1) t*=d;
			d*=d;
			z>>=1;
		}
		return t;
	}
	void work()
	{
		int n,l,s;
		cin>>n>>l>>s;
		vector<vector<ull>> cnt(l);
		vector<ull> powl(l+1);
		powl[0]=1;
		for(int i=1;i<=l;i++) powl[i]=powl[i-1]*131ull;
		for(int _=1;_<=n;_++)
		{
			vector<ull> pre(l+1),suf(l+2);
			string s;
			cin>>s;
			s='+'+s;
			for(int i=1;i<=l;i++) pre[i]=pre[i-1]+((ull)s[i])*powl[i];
			for(int i=l;i>=1;i--) suf[i]=suf[i+1]+((ull)s[i])*powl[i-1];
			for(int i=0;i<l;i++) cnt[i].push_back(pre[i]+suf[i+2]);
		}
		int ans=0;
		for(auto &tmp:cnt)
		{
			sort(tmp.begin(),tmp.end());
			int len=0;
			ull lst=tmp[0];
			for(auto tmp1:tmp)
			{
				if(tmp1==lst) len++;
				else
				{
					lst=tmp1;
					ans+=len*(len-1ll)/2ll;
					len=1;
				}
			}
			ans+=len*(len-1ll)/2ll;
		}
		cout<<ans<<endl;
	}
}
signed main()
{
	ios::sync_with_stdio(0);
	cin.tie(0),cout.tie(0);
	return Qaaxaap::work(),0;
}

